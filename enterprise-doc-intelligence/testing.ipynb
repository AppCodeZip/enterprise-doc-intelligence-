    "\n",
    "model = SentenceTransformer('all-MiniLM-L6-v2')\n",
    "\n",
    "sentences = [\n",
    "    \"Delhi office leave policy\",\n",
    "    \"Bangalore WFH policy\"\n",
    "]\n",
    "\n",
    "embeddings = model.encode(sentences)\n",
    "print(len(embeddings[0]))\n"
   ]
